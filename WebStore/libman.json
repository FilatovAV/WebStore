{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "cdnjs",
      "library": "jquery@2.2.0",
      "destination": "wwwroot/jquery/",
      "files": [
        "jquery.js",
        "jquery.min.js",
        "jquery.min.map"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "font-awesome@4.7.0",
      "destination": "wwwroot/font-awesome/"
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/jquery-validation-unobtrusive/"
    },
    {
      "library": "jquery-validate@1.19.0",
      "destination": "wwwroot/jquery-validate/",
      "files": [
        "jquery.validate.min.js",
        "additional-methods.js",
        "additional-methods.min.js",
        "jquery.validate.js",
        "localization/messages_ru.min.js"
      ]
    },
    {
      "library": "twitter-bootstrap@4.3.1",
      "destination": "wwwroot/lib/twitter-bootstrap/"
    }
  ]
}